			</if>
			<if test="@Ognl@isNotEmpty(messageIdLT)">
				AND t_message_result.message_id &lt; #{messageIdLT}
			</if>
			<if test="@Ognl@isNotEmpty(messageIdGE)">
				AND t_message_result.message_id &gt;= #{messageIdGE}
			</if>
			<if test="@Ognl@isNotEmpty(messageIdLE)">
