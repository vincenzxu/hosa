			</if>
			<if test="@Ognl@isNotEmpty(statusLT)">
				AND t_message_result.status &lt; #{statusLT}
			</if>
			<if test="@Ognl@isNotEmpty(statusGE)">
				AND t_message_result.status &gt;= #{statusGE}
			</if>
			<if test="@Ognl@isNotEmpty(statusLE)">
